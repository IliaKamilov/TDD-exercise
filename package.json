  "bugs": {
    "url": "https://github.com/IliaKamilov/TDD-exercise/issues"
  },
  "homepage": "https://github.com/IliaKamilov/TDD-exercise#readme",
  "description": "",
  "dependencies": {
    "jest": "^29.7.0",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3",
    "@release-it/conventional-changelog": "^10.0.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/node": "^22.13.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
