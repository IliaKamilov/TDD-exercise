    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --ci --coverage --passWithNoTests",
    "release": "release-it --ci",
    "release:dry": "release-it --dry-run",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
