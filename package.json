    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "release-it": "^18.1.2",
    "tailwindcss": "^4.0.8",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
